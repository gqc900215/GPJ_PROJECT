<script>
$(function(){
            $(".clear-condition").hide();
    


    var page = '1';
    var searchstr = '';
    var viewtype = '';
    var cityid = "0";
    var cityname = "全国";
    var column = "" || '';
    if (column == 17){
       $('.free_consult').each(function(i,item){
            var self = $(this);
            $(this).on('click',function(e){
                e.preventDefault();
                self.next().toggle();
            })
        }) 
       $('.free_con_div').each(function(i,item){
            $(this).on('mouseup',function(e){
                return false;
            })
        })

       $(document).mouseup(function(e){
            if($(e.target).parent(".tel").length==0){
                $(".free_con_div").hide();
                $('.error_1').remove();
                $('.php_process_style').each(function(i,item){
                    $(this).val('请输入手机号').css("color", "#bfbfbf");
                })
            }
        });

       $('.php_process_style').each(function(i,item){
            $(this).on('focus',function(e){
                $('.error_1').remove();
                if ($(this).val() == '请输入手机号') {
                    $(this).val("").css("color", "#333");
                } 
            })
            $(this).on('blur',function(e){
                $('.error_1').remove();
                if ($(this).val() == ''){
                    $(this).val('请输入手机号').css("color", "#bfbfbf");
                }
            })
       })

       $('.pop-tel').each(function(i,item){
            var self = $(this);
            $(this).on('click',function(e){
                var carid = $(this).data('carid');
                var mobile = $(this).prev().val();
                var valid = is_valid_mobile(mobile);
                if (valid != 1){
                    if ($(this).prev().hasClass('error_1')){
                        return false;
                    }
                    $(this).before("<a href='javascript:void(0)' class='error_1'>" + valid + "</a>");
                    return false;
                }
                $(".free_con_div").hide();
                show_popup('#creditYes','#creditYes .closeJs');
                $.post('/search/free_call/',{ 'carid':carid, 'client_phone' : mobile , 'client_type' : 3 , type:'ajax' },function(data){
                   var data = JSON.parse(data);
                   if (data.code == 1){
                        $(".free_con_div").hide();
                        self.prev().val('请输入手机号').css("color", "#bfbfbf");    
                   }    
                });
            })
       })
    }   

    if (cityid != 0){
        $.post('/search/get_surround/',{ 'searchstr':searchstr, 'cityid' : cityid, 'page' : page, 'column':column ,type:'ajax' },function(data){
            if (page == 1){
                var data = JSON.parse(data);
                var count = data['count'];
                data = data['data'];
                if (column == 17){
                    process_surround('car-crs','php_saled_car_mark',cityid,cityname,data,count,column);
                }
                else if (viewtype != 1){
                    process_surround('car-vtc','php_saled_car_mark',cityid,cityname,data,count,column);
                }
                else{
                    process_surround('car-crs','php_saled_car_mark',cityid,cityname,data,count,column);
                }
            }
            else{
                if (data < 20){
                    if (($('.car-vtc').length == $('.php_saled_car_mark').length)||($('.car-crs').length == $('.php_saled_car_mark').length)){
                        $('.nber').eq(0).html('');
                    }
                }
            }      
        });
    }
    else{
        if (($('.car-vtc').length == 0)&&($('.car-crs').length == 0)){
            $('.seatch-none').show();
        }
    }

    
    
    
    function process_surround(divclass,saledclass,cityid,cityname,data,count,column){
        if (!data){
            if ($('.seatch-none').length > 0){
                $('.seatch-none').show();
            }
            else
            if ($('.'+saledclass).length == $('.'+divclass).length){
                var content = cityname+"无符合条件车辆，以下是"+cityname+"已售车辆";
                $('.nber').eq(0).html(content);
            }
            else
            if ($('.'+saledclass).length > 0){
                var str = '<p class="sold-car d-icon">以下是' + cityname + '已售车辆</p>';
                $('.'+saledclass).eq(0).before(str);  
            }
        }
        else{
            if ($('.seatch-none').length > 0){
                if (column != 17){
                    var str = '<div class="car-box clearfix">' + data + '</div>';   
                }
                else{
                    var str = '<div class="car-ext clearfix">' + data + '</div>';  
                }
                $('.seatch-none').after(str); 
                var str = cityname + '无符合条件车辆，小优在周边城市为您找到<em>' + count + '</em>辆车';
                $('.round-car').html(str);  
                $('.round-car').css("border-top",0);
            }
            else
            if ($('.'+saledclass).length >0){
                if ($('.'+saledclass).length == $('.'+divclass).length){
                    var content = cityname+"无符合条件车辆，小优在周边城市为您找到<em>"+count+"</em>辆车";
                    $('.nber').eq(0).html(content);
                    var flag = 1;
                }
                $('.'+saledclass).eq(0).before(data);
                if (flag){
                    $('.round-car').hide();
                }
                var str = '<p class="sold-car d-icon">以下是' + cityname + '已售车辆</p>';
                $('.'+saledclass).eq(0).before(str);   
            }
            else
            if ($('.'+divclass).length < 20){
                $('.'+divclass+':last').after(data);
                $('.round-car').css("border-top",0);
            }
        }
    }

    //初始化对比记录
    _init_carcheck(".car-box");
    //初始化用户收藏
    _init_carfavorite(".car-box");
    //增加车系搜索量
    var brandid = "";
    var seriesid_xin = "";
    $.post("/apis/apis_ajax/add_search_series",{ brandid:brandid,seriesid_xin:seriesid_xin });
});
</script></div>
</div>
