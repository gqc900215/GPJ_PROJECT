<script type="text/javascript">
var TOP_INFO = { location:{ } , user:{ } , half_city:[] };
var TOP_GET = { };
TOP_INFO.location = {"cityid":0,"areaid":0,"cityname":"\u5168\u56fd","ename":"quanguo"};

var is_from_wap = /[?|&]from=wap/.test(window.location.search);

//from wap记录cookie
if(is_from_wap){
	$.cookie('access_from', 'wap', { path:'/', expires:0 });
}
if($.cookie('access_from') == 'wap'){
	is_from_wap = true;
}

//手机端自动跳转
if(!is_from_wap && /Android|iP[ao]d|iPhone/gi.test(navigator.userAgent)){
	var reg = new RegExp("/*?" + TOP_INFO.location.ename,"g"); 
	var path = window.location.pathname.replace(reg,'');

	//特别页面跳转
	var con_name = 'search';
	var act_name = 'index';
	var mdomain = 'http://m.xin.com';

	//首页跳转
	if(con_name == 'home' && act_name == 'index' )
		window.location.href = mdomain + path;
	//车辆详情跳转
	else if(con_name == 'car' && act_name == 'index' ) 
		window.location.href = mdomain + path;
	//商铺页跳转
	else if(con_name == 'dealer' && act_name == 'index' ) 
		window.location.href = mdomain + path;
	//文章详情页跳转
	else if(con_name == 'article' && act_name == 'view'){
		var act_arr = window.location.pathname.split('/');
		for(i = act_arr.length-1; i >=0; i--){
			if(!!act_arr[i]) {
				window.location.href = mdomain + '/article/article_detail/' + act_arr[i];
				break;
			}
		}
	}
	//活动列表页面跳转
	else if(con_name == 'article' && act_name == 'listing'){
		var act_arr = window.location.pathname.split('/');
		for(i = act_arr.length-1; i >=0; i--){
			if(!!act_arr[i]) {
				window.location.href = mdomain + '/article/index/' + act_arr[i];
				break;
			}
		}
	}
}
</script>
